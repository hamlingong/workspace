<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:orientation="vertical"
	android:background="@drawable/icob_anima_5bg">
	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:layout_width="fill_parent"
		android:layout_height="40dp">
		<Button
			android:id="@+id/anima_5first"
			android:layout_width="wrap_content"
			android:layout_height="fill_parent"
			android:layout_weight="1"
			android:background="@drawable/icob_anima_5first"/>
		<Button
			android:id="@+id/anima_5pre"
			android:layout_width="wrap_content"
			android:layout_height="fill_parent"
			android:layout_weight="1"
			android:background="@drawable/icob_anima_5pre"/>
		<Button
			android:id="@+id/anima_5replay"
			android:layout_width="wrap_content"
			android:layout_height="fill_parent"
			android:layout_weight="1"
			android:background="@drawable/icob_anima_5replay"/>
		<Button
			android:id="@+id/anima_5next"
			android:layout_width="wrap_content"
			android:layout_height="fill_parent"
			android:layout_weight="1"
			android:background="@drawable/icob_anima_5next"/>
		<Button
			android:id="@+id/anima_5last"
			android:layout_width="wrap_content"
			android:layout_height="fill_parent"
			android:layout_weight="1"
			android:background="@drawable/icob_anima_5last"/>
		<Button
			android:id="@+id/anima_5list"
			android:layout_width="wrap_content"
			android:layout_height="fill_parent"
			android:layout_weight="1"
			android:background="@drawable/icob_anima_5list"/>
		<Button
			android:id="@+id/anima_5power"
			android:layout_width="wrap_content"
			android:layout_height="fill_parent"
			android:layout_weight="1"
			android:background="@drawable/icob_anima_5power"/>
	</LinearLayout>
	<View
		android:id="@+id/anima_5star"
		android:layout_width="90dp"
		android:layout_height="80dp"
		android:layout_marginTop="80dp"
		android:layout_marginLeft="100dp"
		android:background="@drawable/icob_anima_5star"/>
	<TextView
	    android:id="@+id/anima_5msg"
	    android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:layout_gravity="bottom"
		android:gravity="center_horizontal"
		android:textColor="#00FFFF"/>
</LinearLayout>