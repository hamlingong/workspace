<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    style="@style/activitys">
    <SlidingDrawer
        android:id="@+id/list_7sd"
        android:layout_width="fill_parent"
    	android:layout_height="wrap_content"
    	android:orientation="horizontal"
    	android:handle="@+id/list_7pre"
        android:content="@+id/list_lay7">
        <ImageView
            android:id="@+id/list_7pre"
            android:src="@drawable/ico_pre"
            android:layout_width="40dp"
    		android:layout_height="wrap_content"
    		android:contentDescription="@string/comm_image"/>
        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		    android:id="@+id/list_lay7"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:orientation="vertical">
	    </LinearLayout>
    </SlidingDrawer>
</LinearLayout>